/**
 * Checks and sends a form via AJAX
 * @param jQueryObject button The button that was clicked (must have the "js-sendbutton" class)
 * The wrapping div must have the "ajaxform" class and the "data-ajaxurl" attribute (where the data will be sent)
 * The button may have a "data-callback" attribute to send it in the "callback" field
 * The button may have a "data-setvalue" attribute, which will be added to the field having the "data-value" name
 */


window.ajaxformSend = function (button) {
  var form = button.closest('.ajaxform');

  if (!form.validate()) {
    showCustomModal("Please, fill out the mandatory fields"); //form.find('.invalid').first().scrollTo();
  } else {
    form.addClass('js-working');
    var callback = !!button.attr('data-callback') ? "&callback=" + button.attr('data-callback') : ""; // the button may update a field having the name in "data-value" with the value in "data-setvalue"

    if (!!button.attr('data-setvalue')) {
      form.find('input[name=' + button.attr('data-setvalue') + ']').val(button.attr('data-value'));
    }

    var data = "_token=" + $('meta[name="csrf-token"]').attr('content') + "&";
    var method = form.attr('data-method') ? form.attr('data-method').toUpperCase() : "POST"; // for Laravel resource controllers

    if ($.inArray(method, ['DELETE', 'PUT', 'PATCH']) !== -1) {
      data += "_method=" + method + "&";
      method = "POST";
    }

    $.ajax({
      type: method,
      url: form.attr('data-ajaxurl'),
      data: data + form.serializeAnything() + callback,
      dataType: "json",
      error: function error(data) {
        form.removeClass('js-working');

        if (typeof data.responseText != "undefined") {
          var errors = $.parseJSON(data.responseText);
          var html = "";

          if ('onlyText' in errors) {
            html = '<header>' + errors.onlyText + '</header>';
          } else {
            html += "<div class='ajaxform-errors'>";
            html += "<header>Verify your input</header>";
            html += "<ul>";

            for (var field in errors) {
              var input = form.find('input[name^=' + field + '], select[name^=' + field + ']');
              var inputWrapper = input.first().closest('.input-wrapper');

              if (inputWrapper.length) {
                inputWrapper.addClass('invalid');
              } else {
                input.addClass('invalid');
              }

              html += "<li>" + errors[field] + "</li>";
            }

            html += "</ul>";
            html += "</div>";
          }

          showCustomModal(html);
        } else {
          showCustomModal("Invalid request, please try again later");
        }
      },
      success: function success(data) {
        ajaxHandleSuccess(data, form);
      }
    });
  }
};
/**
 * Handles an AJAX requests callback
 * msg {string}  A message to be displayed
 * relaod {bool} If the page is to be reloaded
 * redirect {string} The URL to be redirected to
 * callbackFunction {string} A JS function to be called
 * callbackData {JSON} Data to pass to the JS function
 */


window.ajaxHandleSuccess = function (data, form) {
  if (typeof data.msg != "undefined") {
    // displays a message
    showCustomModal(data.msg, function () {
      ajaxHandleRealSuccess(data, form);
    });
  } else {
    ajaxHandleRealSuccess(data, form);
  }
};

window.showPhoneFormModalResponse = function () {
  showPhoneFormModal();
};

window.ajaxHandleRealSuccess = function (data, form) {
  if (typeof data.reload != "undefined") {
    // reloads the page
    form.find('.gif-working .text').text("The page will be reloaded, please wait");
    window.location.reload();
    return;
  }

  if (typeof data.redirect != "undefined") {
    // redirects the page
    form.find('.gif-working .text').text("Redirecting, please wait");
    window.location.href = data.redirect;
    return;
  }

  if (typeof data.callbackFunction != "undefined") {
    // calls another function
    var callbackData = typeof data.callbackData != "undefined" ? data.callbackData : null;

    if (typeof data.callbackObject != "undefined") {
      window[data.callbackObject][data.callbackFunction](callbackData);
    } else {
      window[data.callbackFunction](callbackData);
    }
  }

  form.removeClass('js-working');
};

window.showCustomModal = function (message, afterCloseAction, src) {
