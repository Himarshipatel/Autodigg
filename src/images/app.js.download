$(document).on("keyup", "textarea.initial-chat-message", function (event) {
  $this = $(this);
  var max_char = $this.attr("maxlength");
  var char_val = $this.val();
  var count_char = $this.val().length;
  var newLines = $this.val().match(/(\r\n|\n|\r)/g);

  if (newLines != null) {
    count_char = count_char + newLines.length;
  }

  var diff = max_char - count_char;

  if (diff < 0) {
    $this.val(char_val.slice(0, -1));
    diff = 0;
  }

  $this.parent().find("p.text-area-char-counter span:first-child").text(diff);
  $this.parent().find("p.text-area-char-counter span:last-child").text(max_char);
});
