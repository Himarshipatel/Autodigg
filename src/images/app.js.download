    $('body').append(html);
  }

  if (src === undefined) {
    src = '#fancy-alert';
  }

  var $modal = $(src);
  messageBlock = $modal.find('div.message');
  messageBlock.html(message);
  $.fancybox.open({
    src: src,
    type: 'inline',
    attr: {
      scrolling: 'no'
    },
    afterClose: function afterClose(instance, current) {
      if (afterCloseAction !== undefined) {
        afterCloseAction();
      }
    }
  });
};

