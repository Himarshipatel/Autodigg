window.hideRequest = function (url, section) {
  $.ajax({
    type: "get",
    url: url,
    dataType: "json",
    error: function error(data) {//console.log(data);
    },
    success: function success(data) {
      if (data.success == 1) {
        showCustomSnackbar(true, data.msg);

        if (section == 'refresh_offer') {
          var url_offer = $(".arrow-steps").find(".step.current").attr("data-offer");

          if (_typeof(url_offer) !== ( true ? "undefined" : undefined) && url_offer !== false) {
            getDealerOffer(url_offer);
          }
        }

        $('#' + section).remove();

        if ($('#invited_data').hasClass('current')) {
          var url_invited = $('#invited_data').attr('data-invited');

          if (_typeof(url_invited) !== ( true ? "undefined" : undefined) && url_invited !== false) {
            getInvitedCar(url_invited);
          }
        }

        if ($('#invited_data').hasClass('current')) {
          var url_invited = $('#invited_data').attr('data-invited');

          if (_typeof(url_invited) !== ( true ? "undefined" : undefined) && url_invited !== false) {
            getInvitedCar(url_invited);
          }
        } else {
          if (!$('.cars-list').length) {
            $('#offers').html('<div class="content-box-dashboard"><div class= "text-center" >This request has not received any offers yet from other dealerships.</div></div>');
          }
        }
      }
    }
  });
};

window.chat_tab = function ($this) {
  var tab_content = $this.attr('aria-controls');
  $('.chat-tab').find('.tab-content .tab-pane').removeClass('active');
  $('.chat-tab').find('.tab-content .tab-pane#' + tab_content).addClass('active');
};

